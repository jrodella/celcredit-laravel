{
  "name": "jrodella/celcredit",
  "type": "library",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laravel/pint": "^1.13.7",
    "phpstan/phpstan": "^1.8",
    "php-http/mock-client": "^1.6",
    "nyholm/psr7": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Celcredit\\": "src/"
    }
  },
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ],
    "refactor": [
      "./vendor/bin/rector --debug"
    ],
    "stan": [
      "./vendor/bin/phpstan analyse src test"
    ],
    "test": [
      "./vendor/bin/phpunit --testdox"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
